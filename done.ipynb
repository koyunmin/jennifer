   "source": [
    "n=list(str(input()).zfill(5))\n",
    "str1=''.join(n[0]+n[1])\n",
    "str2=''.join(n[3]+n[4])\n",
    "H=int(str1)\n",
    "M=int(str2)\n",
    "if (H!=0) or (M!=0):\n",
    "    x=abs(6*M-(30*H+M/2))\n",
    "    if 0<=x<=180:\n",
    "        print(\"%.3f\"%x)\n",
    "    else:\n",
    "        y=abs(360-x)\n",
    "        print(\"%.3f\"%y)\n",
    "        \n",
    "else:\n",
    "    print()\n"
